<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>kannasaverUI</class>
<comment>This is the main UI for setting up your screen saver.</comment>
<author>Ian Reinhart Geiser</author>
<widget class="QDialog">
    <property name="name">
        <cstring>kannasavercfg</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>269</width>
            <height>354</height>
        </rect>
    </property>
    <property name="sizePolicy">
        <sizepolicy>
            <hsizetype>3</hsizetype>
            <vsizetype>3</vsizetype>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
    </property>
    <property name="minimumSize">
        <size>
            <width>269</width>
            <height>354</height>
        </size>
    </property>
    <property name="baseSize">
        <size>
            <width>0</width>
            <height>0</height>
        </size>
    </property>
    <property name="caption">
        <string>Kannasaver Setup</string>
    </property>
    <widget class="QLayoutWidget">
        <property name="name">
            <cstring>layout4</cstring>
        </property>
        <property name="geometry">
            <rect>
                <x>10</x>
                <y>10</y>
                <width>250</width>
                <height>330</height>
            </rect>
        </property>
        <grid>
            <property name="name">
                <cstring>unnamed</cstring>
            </property>
            <widget class="KFontCombo" row="3" column="0">
                <property name="name">
                    <cstring>FontSelector</cstring>
                </property>
                <property name="fontSize" stdset="0">
                    <number>16</number>
                </property>
                <property name="toolTip" stdset="0">
                    <string>Select the font for kana characters</string>
                </property>
                <property name="whatsThis" stdset="0">
                    <string>You can select the font here. Only fonts capable of japanese characters are in this list.</string>
                </property>
            </widget>
            <widget class="QButtonGroup" row="1" column="0">
                <property name="name">
                    <cstring>Use_Characters</cstring>
                </property>
                <property name="title">
                    <string>Use Chararacters</string>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>Hiragana</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>20</y>
                            <width>200</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>H&amp;iragana</string>
                    </property>
                    <property name="accel">
                        <string>Alt+I</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Use Hiragana</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Select Hiragana characters.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>Katakana</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>45</y>
                            <width>210</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>&amp;Katakana</string>
                    </property>
                    <property name="accel">
                        <string>Alt+K</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>use Katakana</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Select Katakana characters.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>Mixed</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>70</y>
                            <width>210</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>Bo&amp;th</string>
                    </property>
                    <property name="accel">
                        <string>Alt+T</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Mix Hiragana and Katakana</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Randomly mix Hiragana and Katakana.</string>
                    </property>
                </widget>
            </widget>
            <widget class="QLayoutWidget" row="4" column="0">
                <property name="name">
                    <cstring>layout5</cstring>
                </property>
                <hbox>
                    <property name="name">
                        <cstring>unnamed</cstring>
                    </property>
                    <spacer>
                        <property name="name">
                            <cstring>spacer4</cstring>
                        </property>
                        <property name="orientation">
                            <enum>Qt::Horizontal</enum>
                        </property>
                        <property name="sizeType">
                            <enum>Expanding</enum>
                        </property>
                        <property name="sizeHint">
                            <size>
                                <width>40</width>
                                <height>20</height>
                            </size>
                        </property>
                    </spacer>
                    <widget class="QPushButton">
                        <property name="name">
                            <cstring>OkayPushButton</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;OK</string>
                        </property>
                        <property name="accel">
                            <string>Alt+O</string>
                        </property>
                    </widget>
                    <widget class="QPushButton">
                        <property name="name">
                            <cstring>CancelPushButton</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Cancel</string>
                        </property>
                        <property name="accel">
                            <string>Alt+C</string>
                        </property>
                    </widget>
                </hbox>
            </widget>
            <widget class="QButtonGroup" row="0" column="0">
                <property name="name">
                    <cstring>buttonGroup2</cstring>
                </property>
                <property name="sizePolicy">
                    <sizepolicy>
                        <hsizetype>3</hsizetype>
                        <vsizetype>3</vsizetype>
                        <horstretch>0</horstretch>
                        <verstretch>0</verstretch>
                    </sizepolicy>
                </property>
                <property name="title">
                    <string>Mode</string>
                </property>
                <property name="exclusive">
                    <bool>true</bool>
                </property>
                <property name="toolTip" stdset="0">
                    <string></string>
                </property>
                <property name="whatsThis" stdset="0">
                    <string></string>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>ExtendedChars</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>45</y>
                            <width>205</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>Basic + E&amp;xtended Chars</string>
                    </property>
                    <property name="accel">
                        <string>Alt+X</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Basic + extended characters</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Select basic and extended characters.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>CompoundChars</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>70</y>
                            <width>205</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>Basic + Extended + Com&amp;pound Chars</string>
                    </property>
                    <property name="accel">
                        <string>Alt+P</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Basic, extended and compound characters</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Select all characters, basic, extended and compound.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>BasicChars</cstring>
                    </property>
                    <property name="geometry">
                        <rect>
                            <x>10</x>
                            <y>20</y>
                            <width>205</width>
                            <height>19</height>
                        </rect>
                    </property>
                    <property name="text">
                        <string>&amp;Basic Chars</string>
                    </property>
                    <property name="accel">
                        <string>Alt+B</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Basic characters</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Select only the basic 46 characters.</string>
                    </property>
                </widget>
            </widget>
            <widget class="QLabel" row="2" column="0">
                <property name="name">
                    <cstring>FontSample</cstring>
                </property>
                <property name="frameShape">
                    <enum>Box</enum>
                </property>
                <property name="frameShadow">
                    <enum>Sunken</enum>
                </property>
                <property name="text">
                    <string>ひらがな カタカナ 日本語</string>
                </property>
                <property name="scaledContents">
                    <bool>true</bool>
                </property>
                <property name="alignment">
                    <set>Qt::AlignCenter</set>
                </property>
                <property name="toolTip" stdset="0">
                    <string>This is a font sample for the font selected below. it reads "Hiragana Katakana Nihongo"</string>
                </property>
            </widget>
        </grid>
    </widget>
</widget>
<connections>
    <connection>
        <sender>FontSelector</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>kannasavercfg</receiver>
        <slot>FontChanged()</slot>
    </connection>
</connections>
<includes>
    <include location="global" impldecl="in implementation">kdialog.h</include>
</includes>
<slots>
    <slot>FontChanged()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
<layoutfunctions spacing="KDialog::spacingHint" margin="KDialog::marginHint"/>
<includehints>
    <includehint>kfontcombo.h</includehint>
    <includehint>klineedit.h</includehint>
</includehints>
</UI>
